<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.serb.podpamp"
		  android:versionCode="1"
		  android:versionName="1.0">
	<uses-sdk android:minSdkVersion="8"/>
	<uses-permission android:name="android.permission.INTERNET"/>
	<application android:label="@string/app_name">
		<activity android:name="com.serb.podpamp.ui.MainActivity"
				  android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".ui.FeedsActivity"
				  android:label="@string/feeds_list_title"/>
		<activity android:name=".ui.FeedItemsActivity"/>
		<provider
				android:name="com.serb.podpamp.model.provider.FeedsProvider"
				android:authorities="com.serb.podpamp"
				android:exported="false"/>
		<service
				android:name="com.serb.podpamp.model.request.FeedsService"
				android:exported="false"/>
	</application>
</manifest>
